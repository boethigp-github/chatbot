{
  "app_name": "MyApp",
  "app_env": "production",
  "debug": false,
  "default": {
    "SERVER_URL": "${SERVER_URL:http://localhost:5000}",
    "DEBUG": "${DEBUG:false}",
    "ENV": "${ENV:development}",
    "APP_LOG_LEVEL": "${APP_LOG_LEVEL:DEBUG}",
    "APP_PROJECT_NAME": "${APP_PROJECT_NAME:HUDINI}",
    "TESTING": "${TESTING:true}",
    "SWAGGER_URL": "${SWAGGER_URL:/api/docs}",
    "API_URL": "${API_URL:/swagger.yaml}",
    "API_KEY_OPEN_AI": "${API_KEY_OPEN_AI:''}",
    "API_KEY_ANTHROPIC": "${API_KEY_ANTHROPIC:''}",
    "PROJECT_MODEL_PATH": "${PROJECT_MODEL_PATH:C:\\projects\\llama.cpp\\models\\custom\\}",
    "DATABASE_URL": "${DATABASE_URL:postgresql+asyncpg://postgres:postgres@localhost/hudini}",
    "SQLALCHEMY_TRACK_MODIFICATIONS": "${SQLALCHEMY_TRACK_MODIFICATIONS:false}",
    "CORS_ORIGINS": "${CORS_ORIGINS:http://localhost:5173,https://editor.swagger.io}",
    "LOGGING_CONFIG": {
      "version": 1,
      "disable_existing_loggers": false,
      "formatters": {
        "default": {
          "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
      },
      "handlers": {
        "console": {
          "class": "logging.StreamHandler",
          "formatter": "default"
        }
      },
      "root": {
        "handlers": ["console"],
        "level": "${LOG_LEVEL:DEBUG}"
      },
      "loggers": {
        "uvicorn": {
          "level": "INFO"
        },
        "myapp": {
          "level": "DEBUG",
          "handlers": ["console"],
          "propagate": false
        }
      }
    }
  },
  "APP_PORT": "${APP_PORT:5000}"
}